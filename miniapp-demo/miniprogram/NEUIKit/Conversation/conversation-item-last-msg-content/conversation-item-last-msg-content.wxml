<view class="last-msg-content">
  <view wx:if="{{lastMessageState === 1}}" class="content-desc">
    {{recallText}}
  </view>
  <view wx:elif="{{messageType === 'notification'}}" class="content-desc">
    {{notiMsgText}}
  </view>
  <view wx:elif="{{sendingState === 3}}" class="content-desc">
    {{sendFailText}}
  </view>
  <view wx:elif="{{messageType === 'text'}}" class="content-desc">
    {{lastMessage.text}}
  </view>
  <view wx:elif="{{messageType === 'image'}}" class="content-desc">
    [{{imgMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'audio'}}" class="content-desc">
    [{{audioMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'video'}}" class="content-desc">
    [{{videoMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'file'}}" class="content-desc">
    [{{fileMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'location'}}" class="content-desc">
    [{{geoMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'avchat'}}" class="content-desc">
    [{{callMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'tips'}}" class="content-desc">
    [{{tipMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'robot'}}" class="content-desc">
    [{{robotMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'call'}}" class="content-desc">
    [{{callMsgText}}]
  </view>
  <view wx:elif="{{messageType === 'custom'}}" class="content-desc">
    [{{customMsgText}}]
  </view>
  <view wx:else class="content-desc">
    [{{unknowMsgText}}]
  </view>
</view>