<view class="search-result-list-item" bindtap="handleItemClick">
  <view class="result-item-avatar">
    <avatar account="{{to}}" avatar="{{item.avatar}}"></avatar>
  </view>
  <view wx:if="{{!isTeam}}" class="result-item-title">
    <appellation account="{{to}}"></appellation>
    <view class="result-item-account">{{to}}</view>
  </view>
  <view wx:else class="result-item-title">
    {{teamName}}
  </view>
</view>